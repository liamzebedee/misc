<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.emonic.base.wizards.NewProjectWizard.actionSet2"
            label="New Wizard Action Set"
            visible="true">
         <action
               class="org.emonic.base.wizards.importprojectfswizard.NewDotNetProjectActionDelegate"
               icon="icons/newproject_wiz.gif"
               id="org.emonic.base.wizards.ImportFSProjectWizard.action1"
               label="Import  .NET Project from FileSystem"
               state="true"
               style="push"
               toolbarPath="Normal/CSharpWizards">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="emonic.dotNet"
            class="org.emonic.base.wizards.importprojectfswizard.ImportDotNetProjectResourceWizard"
            icon="icons/newproject_wiz.gif"
            id="org.emonic.base.wizards.ImportProjectFSWizard.wizard1"
            name=".Net Import Project from Filesystem">
      </wizard>
   </extension>

</plugin>
